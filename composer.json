{
    "name": "talesoft/tale-dev-tool",
    "type": "library",
    "keywords": ["tale", "dev tools"],
    "description": "Talesoft unit tests, coverage and code style tools",
    "license": "MIT",
    "homepage": "http://talesoft.codes",
    "authors": [
        {
            "name": "Torben Koehn",
            "email": "torben@talesoft.codes"
        },
        {
            "name": "KyleKatarn",
            "email": "jade-php@selfbuild.fr",
            "homepage": "http://github.com/kylekatarnls"
        }
    ],
    "support": {
        "email": "torben@talesoft.codes",
        "issues": "https://github.com/Talesoft/tale-dev-tool/issues",
        "source": "https://github.com/Talesoft/tale-dev-tool"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.0 || ^7.0",
        "phpunit/php-code-coverage": "^2.2 || ^4.0 || ^5.2 || ^6.0",
        "squizlabs/php_codesniffer": "^2.8",
        "symfony/console": "^3.2",
        "codeclimate/php-test-reporter": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Tale\\": "./src"
        }
    },
    "bin": ["bin/tale-dev"],
    "suggest": {}
}
